<div id="account_payment_service" class="col-md-12">
		
	<table class="table table-bordered table-striped">
	</table>
	
	<table class="table table-bordered table-striped">
		
		<tr>
			<th width="10%">ID</th>
			<th width="10%">Account</th>
			<th width="10%">Name</th>
			<th width="10%">Address 1</th>
			<th width="10%">Address 2</th>
			<th width="10%">City</th>
			<th width="10%">State</th>
			<th width="10%">Zip</th>
			<th width="10%">Phone</th>
			<th width="10%">Fax</th>
			<th width="10%">Main</th>
		</tr>
		
		<tr>
