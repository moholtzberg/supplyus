<fieldset>  
  
  <div class="col-md-12">
    <%= f.label :first_name, :class => "control-label" %>
    <%= f.text_field :first_name, :class => "form-control disabled" %>
  </div>
	
  <div class="col-md-12">
    <%= f.label :last_name, :class => "control-label" %>
    <%= f.text_field :last_name, :class => "form-control disabled" %>
  </div>
	
  <div class="col-md-12">
    <%= f.label :email, :class => "control-label" %>
    <%= f.text_field :email, :class => "form-control disabled" %>
  </div>
	
  <div class="col-md-12">
    <%= f.label :phone_number, :class => "control-label" %>
    <%= f.text_field :phone_number, :class => "form-control disabled" %>
  </div>
	
  <div class="col-md-12">
    <%= f.label :password, :class => "control-label" %>
    <%= f.password_field :password, :class => "form-control disabled" %>
  </div>
	
  <div class="col-md-12">
    <%= f.label :password_confirmation, :class => "control-label" %>
    <%= f.password_field :password_confirmation, :class => "form-control disabled" %>
  </div>
  
  <%= f.hidden_field :account_id, :value => @account.id %>
  
</fieldset>