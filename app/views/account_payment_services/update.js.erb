<% if @account_payment_service.errors.any? %>
  $(".modal-content").html("<%= j render :template => 'account_payment_services/edit.html', :format => 'html' %>")
<% else %>
  $('#myModal').modal("hide")
  $('body').removeClass('modal-open')
  $('.modal-backdrop').remove()
  $("#account_payment_services").html("<%= j render :template => 'account_payment_services/index.html', :format => 'html' %>")
<% end %>