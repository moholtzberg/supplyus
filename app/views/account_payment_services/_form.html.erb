<fieldset>	
	
	<div id="name" class="col-md-12">
		<%= f.label :name, :class => "control-label" %>
		<%= f.text_field :name, :class => "form-control" %>
	</div>
	
	<div id="service_id" class="col-md-12">
		<%= f.label :service_id, :class => "control-label" %>
		<%= f.text_field :service_id, :class => "form-control" %>
	</div>
	
	<div id="account" class="col-md-12">
		<%= f.label :account, :class => "control-label" %>
		<%= f.text_field :account_name, :class => "form-control", data: {autocomplete_source: accounts_path} %>
	</div>
	
	<script>
		jQuery(function() {
			return $('#account_payment_service_account_name').autocomplete({
				source: $('#account_payment_service_account_name').data('autocomplete-source')
			})
		});
	</script>	
	
</fieldset>