<!DOCTYPE html>
<html>
	
	<head>
		<title><%= yield :title %></title>
		<%= stylesheet_link_tag    'shop', media: 'all', 'data-turbolinks-track' => 'reload' %>
		<%= javascript_include_tag 'application', 'data-turbolinks-track' => 'reload' %>
		<%= csrf_meta_tags %>
		<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/jasny-bootstrap/3.1.3/css/jasny-bootstrap.min.css">
		<script src="//cdnjs.cloudflare.com/ajax/libs/jasny-bootstrap/3.1.3/js/jasny-bootstrap.min.js"></script>
		<link href="//cdnjs.cloudflare.com/ajax/libs/select2/4.0.1/css/select2.min.css" rel="stylesheet" />
		<script src="//cdnjs.cloudflare.com/ajax/libs/select2/4.0.1/js/select2.min.js"></script>
		<meta name="viewport" content="width=device-width, initial-scale=1">
	</head>
	
	<body class="florida">

		<div id="main" class="container" >

			<header id="header" data-hook>
				
				<div id="nav_bar" class="row hidden-xs hidden-sm">
					
					
					<div class="header-top-left">
						
						<ul class="list-inline">
						
							<li>&nbsp;</li>
							
						</ul>
						
					</div>
					
				</div>
				
				<div id="main_nav_bar" class="row">
					
					<div id="header_banner" class="container">
						
						<div id="MainLogo" class="col-xs-12 col-sm-4 col-md-3">
							
							<a href="/"><h4><strong>24/7 Office Supply</strong></h4><%# image_tag("copiersflorida_logo.png", width: "300", class: "logo") %></a>
							
						</div>
						
					</div>
					
					<nav id="main_nav" class="navbar navbar-default" role="navigation">
						
					</nav>
					
				</div>
				
			</header>

			<div id="content" class="row" data-hook>

				<div id="center-panel" class="col-md-<%= !content_for?(:sidebar) ? "12" : "10" %>" data-hook>

					<%# flash_messages %>

					<%= yield %>

				</div>

			</div>

			<%# render :partial => 'spree/shared/footer' %>

		</div>

	</body>
	
</html>