<div class="modal-header">
	<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
	<h4 class="modal-title" id="myModalLabel">New Item Item List</h4>
</div>

<div class="modal-body" id="modal-body">
	
	<% if @item_item_list.errors.any? %>
		<div id="flash_messages" class="alert alert-danger" role="alert">
			<strong><%= @item_item_list.errors.full_messages.join(', ') %></strong>
		</div>
	<% end %>
	
	
	<%= render :partial => "item_item_lists/form", :locals => {:item_item_list => @item_item_list}%>

</div>

<div class="modal-footer">
	<%= button_tag(:name => :commit, :type => :submit, :class => "btn btn-primary", :form => "new_item_item_list") do %>
		Create Item Item List
	<% end %>
</div>
